#!/usr/bin/env bash

EPOCH="$HOME/.garden/presence/epoch/epoch.txt"
STATE="$HOME/.garden/presence/epoch/state.txt"

{
  echo "Epoch Engine"
  echo "============"
  echo ""
  echo "Epoch:"
  cat "$EPOCH"
  echo ""
  echo "State:"
  cat "$STATE"
} > "$HOME/.garden/lantern/views/epoch.txt"

echo "ðŸŒ¿ Epoch displayed."
