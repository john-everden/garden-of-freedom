#!/usr/bin/env bash

SEASON="$HOME/.garden/presence/season/season.txt"
STATE="$HOME/.garden/presence/season/state.txt"

{
  echo "Season Engine"
  echo "============="
  echo ""
  echo "Season:"
  cat "$SEASON"
  echo ""
  echo "State:"
  cat "$STATE"
} > "$HOME/.garden/lantern/views/season.txt"

echo "ğŸŒ¿ Season displayed."

