#!/usr/bin/env bash
MYTH="$HOME/.garden/presence/mythos/mythos.txt"
STATE="$HOME/.garden/presence/mythos/state.txt"

{
  echo "Mythos Engine"
  echo "============="
  echo ""
  echo "Mythos:"
  cat "$MYTH"
  echo ""
  echo "State:"
  cat "$STATE"
} > "$HOME/.garden/lantern/views/mythos.txt"

echo "ðŸŒ¿ Mythos displayed."
