#!/usr/bin/env bash
COS="$HOME/.garden/presence/cosmos/cosmos.txt"
STATE="$HOME/.garden/presence/cosmos/state.txt"

{
  echo "Cosmos Engine"
  echo "============="
  echo ""
  echo "Cosmos:"
  cat "$COS"
  echo ""
  echo "State:"
  cat "$STATE"
} > "$HOME/.garden/lantern/views/cosmos.txt"

echo "ðŸŒ¿ Cosmos displayed."
